{
  "name": "Log",
  "version": "1.0.4",
  "description": "A log and time-tracking system",
  "repository": "https://github.com/joshavanier/log",
  "main": "./main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron . dev",
    "build_osx": "electron-packager . Log --platform=darwin --arch=x64 --out ./build --overwrite; echo 'Built for macOS'",
    "build_linux": "electron-packager . Log --platform=linux  --arch=x64 --out ./build --overwrite; echo 'Built for Linux'",
    "build_win": "electron-packager . Log --platform=win32  --arch=x64 --out ./build --overwrite; echo 'Built for Windows'"
  },
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-packager": "^8.4.0",
    "electron-reload": "^1.2.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.2",
    "gulp-if": "^2.0.2"
  },
  "dependencies": {
    "headless-work-timer": "0.0.3",
    "aequirys": "1.1.1"
  }
}
